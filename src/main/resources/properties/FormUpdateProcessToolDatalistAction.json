                "value" : "false",
                "label" : "False"
            },{
                "value" : "true",
                "label" : "True"
            }]
         },
         {
             "name" : "delay",
             "label" : "@@org.joget.marketplace.FormUpdateProcessToolDatalistAction.delay@@",
             "type":"textfield",
             "value":"0",
             "description":"@@org.joget.marketplace.FormUpdateProcessToolDatalistAction.delay.desc@@",
             "required":"true",
             "regex_validation" : "^[0-9]+$"
         }
      ]
   }
